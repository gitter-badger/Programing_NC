PROC SCREW_W SAVE DISPLON
;外螺纹磨削

DRESS_STA=$A_DBB[0]
IF DRESS_STA==1
    DRESSER
    RET
ENDIF

OPERA_STA=$A_DBB[1]
IF OPERA_STA==1
    OPERATION
    RET
ENDIF

IF TECH_TIME_R+TECH_TIME_MR+TECH_TIME_MF+TECH_TIME_F==0
    RET
ENDIF

S_PAR_INT
WHILE(TECH_TIME_RCURNT+TECH_TIME_MRCURNT+TECH_TIME_MFCURNT+TECH_TIME_FCURNT<=TECH_TIME_R+TECH_TIME_MR+TECH_TIME_MF+TECH_TIME_F)
    STOPRE
    S_LINESPEED_GRIND;
    S_CYCLE; 
    BCYC;
ENDWHILE

RET